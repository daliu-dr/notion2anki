{
  "name": "notion2anki",
  "description": "Go from Notion toggle list to Anki Cards easily!",
  "keywords": [
    "notion",
    "anki",
    "notion2anki",
    "imba",
    "electron"
  ],
  "author": "Alexander Alemayhu",
  "version": "0.1.26-0",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --mode=development --content-base dist/",
    "watch": "webpack --watch --progress --colors --display-modules",
    "test": "imba test/create_deck.imba",
    "build": "imba build.imba",
    "start": "yarn dev",
    "build-server": "yarn run imbac src/server/server.imba -o .",
    "watch-server": "yarn run imbac -w src/server/server.imba -o .",
    "dev-server": "yarn run imba src/server/server.imba"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alemayhu/notion2anki.git"
  },
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "genanki": "^0.1.1",
    "imba": "^2.0.0-alpha.70",
    "jszip": "^3.5.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.1"
  },
  "build": {
    "appId": "com.alemayhu.notion2anki",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "main.js",
      "dist/app.js",
      "dist/index.html",
      "build",
      "node_modules"
    ]
  }
}
